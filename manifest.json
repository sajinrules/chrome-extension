{
  "manifest_version": 2,

  "name": "English Malayalam Dictionary",
  "description": "This extension shows a Malayalam meaning of selected word",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "icons": {
          "16": "icon.png"
  },
  "background": {
    "scripts": ["sample.js"]
  }
}
